{
  "name": "ts-junit",
  "version": "1.0.9",
  "description": "use junit descrator with typescript",
  "main": "dist/index.js",
  "sideEffects": false,
  "types": "./types/index.d.ts",
  "bin": {
    "junit": "dist/bin.js"
  },
  "scripts": {
    "start": "ts-node ./src/bin.ts",
    "build": "rm -rf ./dist && tsc",
    "test": "uvu -r ts-node/register test -i fixtures",
    "dev": "ts-node test.ts",
    "cli": "ts-node src/bin.ts  tests/test.ts",
    "api": "ts-node api.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i5ting/ts-junit.git"
  },
  "keywords": [
    "uvu",
    "jest",
    "test",
    "tdd",
    "junit",
    "ts",
    "node",
    "cli",
    "ava",
    "mocha",
    "tape",
    "qunit"
  ],
  "author": "i5ting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i5ting/ts-junit/issues"
  },
  "homepage": "https://github.com/i5ting/ts-junit#readme",
  "dependencies": {
    "bluebird": "^3.7.2",
    "debug": "^4.3.1",
    "recast": "^0.21.5",
    "typescript": "^4.8.3",
    "uvu": "^0.5.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.13",
    "concurrently": "^5.3.0",
    "git-cz": "^4.9.0",
    "ts-node": "^10.9.1"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
