{
  "name": "ts-junit",
  "version": "1.0.18",
  "description": "use junit decorator with typescript",
  "main": "dist/index.js",
  "sideEffects": false,
  "typings": "./dist/junit.d.ts",
  "bin": {
    "junit": "dist/bin.js"
  },
  "scripts": {
    "start": "ts-node ./src/bin.ts",
    "gen-types": "rm -rf ./types && tsc -p ./config/tsconfig.json",
    "build": "rm -rf ./dist && tsc && npm run gen-types && npm run api-extractor",
    "release": "npm test && npm build && np",
    "test": "uvu -r ts-node/register test -i fixtures",
    "dev": "ts-node test.ts",
    "cli": "ts-node src/bin.ts  examples/test.ts",
    "api": "ts-node api.ts",
    "api-extractor": "api-extractor run --local",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i5ting/ts-junit.git"
  },
  "keywords": [
    "uvu",
    "jest",
    "test",
    "tdd",
    "junit",
    "ts",
    "node",
    "cli",
    "ava",
    "mocha",
    "tape",
    "qunit"
  ],
  "author": "i5ting",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/i5ting/ts-junit/issues"
  },
  "homepage": "https://github.com/i5ting/ts-junit#readme",
  "dependencies": {
    "bluebird": "^3.7.2",
    "debug": "^4.3.1",
    "recast": "^0.21.5",
    "typescript": "^4.8.3",
    "uvu": "^0.5.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.37",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.7.18",
    "@types/yargs": "^17.0.13",
    "husky": "^8.0.1",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1"
  },
  "files": [
    "dist",
    "types",
    "README.md"
  ]
}
